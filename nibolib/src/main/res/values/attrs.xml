<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomBottomSheetBehavior">
        <attr name="anchorPoint" format="dimension" />
    </declare-styleable>


    <declare-styleable name="RoundedView">
        <attr name="colorSelected" format="color" />
        <attr name="colorUnselected" format="color" />
    </declare-styleable>


    <attr name="originEditTextBg" format="reference|color" />
    <attr name="destEditTextBg" format="reference|color" />

    <attr name="origTextViewHint" format="string|reference" />
    <attr name="destTextViewHint" format="string|reference" />

    <attr name="origTextViewTextColor" format="string|reference" />
    <attr name="destTextViewTextColor" format="string|reference" />

    <attr name="origTextViewHintColor" format="color|reference" />
    <attr name="destTextViewHintColor" format="color|reference" />


    <attr name="distanceDurationTextColor" format="color|reference" />
    <attr name="originDestinationTextColor" format="color|reference" />


    <attr name="suggestionItemTitleTextColor" format="color|reference" />
    <attr name="suggestionItemSubtitleTextColor" format="color|reference" />
    <attr name="suggestionItemSeperatorLineColor" format="color|reference" />
    <attr name="suggestionItemLeftIconTintColor" format="color|reference" />


    <attr name="textViewClearerIcon" format="reference" />

    <attr name="detailsBackground" format="color|reference" />
    <attr name="appBarBackground" format="color|reference"/>

    <attr name="doneFabIconRes" format="reference" />
    <attr name="doneFabColorRes" format="color|reference" />

    <attr name="destinationRoundViewStyle" format="reference" />
    <attr name="originRoundViewStyle" format="reference" />

    <attr name="originDestinationSeperatorLineColor" format="color|reference" />

    <attr name="progressBarColor" format="color|reference" />


    <declare-styleable name="NiboPlacesAutoCompleteSearchView">
        <attr name="niboSV_searchTextColor" format="color" />
        <attr name="niboSV_logoDrawable" format="reference" />
        <attr name="niboSV_logoString" format="string" />
        <attr name="niboSV_editTextColor" format="color" />
        <attr name="niboSV_editHintText" format="string" />
        <attr name="niboSV_editHintTextColor" format="color" />
        <attr name="niboSV_searchCardElevation" format="dimension" />
        <attr name="niboSV_displayMode" format="enum">
            <enum name="menuItem" value="0" />
            <enum name="toolbar" value="1" />
        </attr>
        <attr name="niboSV_homeButtonColor" format="color" />
        <attr name="niboSV_customToolbarHeight" format="dimension" />
        <attr name="niboSV_homeButtonMode" format="enum">
            <enum name="arrow" value="0" />
            <enum name="burger" value="1" />
        </attr>
    </declare-styleable>

</resources>