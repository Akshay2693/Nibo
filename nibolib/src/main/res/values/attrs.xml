<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomBottomSheetBehavior">
        <attr name="anchorPoint" format="dimension" />
    </declare-styleable>


    <declare-styleable name="RoundedView">
        <attr name="colorSelected" format="color" />
        <attr name="colorUnselected" format="color" />
    </declare-styleable>


    <attr name="niboODPicker_originTextViewBg" format="reference" />
    <attr name="niboODPicker_destTextViewBg" format="reference" />
    <attr name="niboODPicker_origTextViewHint" format="string" />
    <attr name="niboODPicker_destTextViewHint" format="string" />
    <attr name="niboODPicker_distanceDurationTextColor" format="color" />
    <attr name="niboODPicker_originDestinationTextColor" format="color" />
    <attr name="niboODPicker_textViewClearerIcon" format="reference" />
    <attr name="niboODPicker_detailsBackgroundColor" format="color" />
    <attr name="niboODPicker_doneFabIconRes" format="reference" />
    <attr name="niboODPicker_doneFabColorRes" format="color" />
    <attr name="niboODPicker_destinationRoundViewCheckedColor" format="color" />
    <attr name="niboODPicker_destinationRoundViewUncheckedColor" format="color" />
    <attr name="niboODPicker_originRoundViewCheckedColor" format="color" />
    <attr name="niboODPicker_originRoundViewUncheckedColor" format="color" />
    <attr name="niboODPicker_progressBarColor" format="color" />


    <declare-styleable name="NiboPlacesAutoCompleteSearchView">
        <attr name="niboSV_searchTextColor" format="color" />
        <attr name="niboSV_logoDrawable" format="reference" />
        <attr name="niboSV_logoString" format="string" />
        <attr name="niboSV_editTextColor" format="color" />
        <attr name="niboSV_editHintText" format="string" />
        <attr name="niboSV_editHintTextColor" format="color" />
        <attr name="niboSV_searchCardElevation" format="dimension" />
        <attr name="niboSV_displayMode" format="enum">
            <enum name="menuItem" value="0" />
            <enum name="toolbar" value="1" />
        </attr>
        <attr name="niboSV_homeButtonColor" format="color" />
        <attr name="niboSV_customToolbarHeight" format="dimension" />
        <attr name="niboSV_homeButtonMode" format="enum">
            <enum name="arrow" value="0" />
            <enum name="burger" value="1" />
        </attr>
    </declare-styleable>

</resources>